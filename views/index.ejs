        <div class="d-flex justify-content-end">
          <table class="table w-25">
            <tbody class="fw-bold">
              <tr class="table-border-color">
                <td>Total Charge</td>
                <td>772.1006 AED</td>
              </tr>
            </tbody>
          </table>
        </div>
      </div>
    </div>

    <div class="mb-5">
      <h3 class="mb-3">Destination Charges</h3>
      <div class="shadow">
        <table class="table">
          <thead>
            <tr>
              <th class="bg-success text-light" scope="col">Country</th>
              <th class="bg-success text-light" scope="col">City</th>
              <th class="bg-success text-light" scope="col">Airport</th>
